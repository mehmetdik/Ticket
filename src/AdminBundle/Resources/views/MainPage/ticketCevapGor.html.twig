<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" aria-hidden="true" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="myModalLabel">Ticket Cevabı</h4>
        </div>
        <div class="modal-body">

            <div class="form-group">
                <label for="exampleInputEmail1">Başlık :</label><br>
                {{ Ticket.baslik|default('') }}
            </div>


            <div class="form-group">
                <label for="exampleInputEmail1">Metin :</label><br>
                {{ Ticket.metin|default('') }}
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">Cevap :</label><br>
                {{ cevap.metin|default('') }}
            </div>


        </div>
    </div>
</div>